{
  "version": 2,

  "routes": [
    {
      "src": "/libs/(.*)\\.wasm",
      "headers": {
        "Content-Type": "application/wasm",
        "Cache-Control": "public, max-age=0, must-revalidate",
        "Content-Encoding": "identity"
      }
    }
  ],

  "rewrites": [
    {
      "source": "/((?!libs/).*)",
      "destination": "/index.html"
    }
  ]
}




