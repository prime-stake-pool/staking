{
  "version": 2,

  "headers": [
    {
      "source": "/libs/(.*)\\.wasm",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" },
        { "key": "Content-Encoding", "value": "identity" }
      ]
    }
  ],

  "overrides": {
    "libs/*.wasm": {
      "compression": false
    }
  },

  "rewrites": [
    {
      "source": "/((?!libs/).*)",
      "destination": "/index.html"
    }
  ]
}


