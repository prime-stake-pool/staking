{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/libs/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/wasm"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "mimeTypes": {
    "wasm": "application/wasm"
  }
}

