{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "main.js", "use": "@vercel/static" },
    { "src": "libs/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/libs/(.*)", "dest": "/libs/$1" },
    { "src": "/main.js", "dest": "/main.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

